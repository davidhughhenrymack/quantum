

include "cpu";


print "Testing fetch()";

qureg a[width];
qureg dest[width];
qureg mem[cells * width];
qureg o[cells];
int rOut;
int i;
int j;
int tst;
int fail;

int memIn;
int memOut;


for i = 0 to cells-1
{
	// if i != 0 {
	// 	quickrand(mem);
	// }
	set(2, mem);
	set(i, a);
	
	measure seek(mem,i), tst;
	measure mem, memIn;
	
	fetch(a, mem, dest);
	dump o;

	measure dest, rOut;
	// !fetchpll(a, mem, dest);
	// dump mem;

	measure mem, memOut;

	if rOut == tst and memOut == memIn {
		print i, "Pass:", rOut, "=", tst, memOut, "=", memIn;
	} else {
		print i, "Fail:", rOut, "=", tst, memOut, "=", memIn;
		fail = fail + 1;
	}

	reset;
}

if fail > 0 {
	print "Failed" , fail, "times";
} else {
	print "Passed";
}
